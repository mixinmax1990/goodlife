<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingTop="0dp"
    android:elevation="100dp"
    android:outlineProvider="none"
    android:id="@+id/graph_detail_container">
    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp"
        android:id="@+id/frame"
        app:backgroundCustomColor="#1a1b1c"
        app:borderCustomRadius="50"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="120dp"
        android:text="Income"
        android:textSize="11sp"
        android:id="@+id/pop_incometitle"
        android:scaleX="2"
        android:scaleY="2"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/pop_incometitle"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="+180,46"
        android:scaleY="2"
        android:scaleX="2"
        android:textSize="16sp"
        android:id="@+id/pop_incomevalue"/>





</androidx.constraintlayout.widget.ConstraintLayout>