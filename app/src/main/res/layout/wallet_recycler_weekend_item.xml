<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="30dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:text="Week Analysis"
        android:textSize="26sp"
        android:id="@+id/week_analysis_title"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kalendar Week 41"
        android:id="@+id/kweek_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/week_analysis_title"
        android:alpha=".5"/>
    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:id="@+id/weekend_analysis_card"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="3:5"
        app:layout_constraintTop_toBottomOf="@id/kweek_title"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:borderCustomRadius="50"
        >

    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <!--<com.news.goodlife.CustomViews.BarChartHorizontal
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/weekendchart"
        app:layout_constraintStart_toStartOf="@id/weekend_analysis_card"
        app:layout_constraintEnd_toEndOf="@id/weekend_analysis_card"
        app:layout_constraintTop_toBottomOf="@id/weeken_analysis_title"
        app:layout_constraintBottom_toTopOf="@id/extra_weekend_analysis_data"/>
