<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="?attr/baseBackground"
    tools:context=".MainActivity">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Today"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="16sp"
        android:visibility="gone"
        android:textColor="?attr/textColorPrimary"
        android:id="@+id/wallet_overview_today_title"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Calendar"
        android:textColor="?attr/textColorPrimary"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="16sp"
        android:visibility="gone"
        android:id="@+id/wallet_overview_calendar_title"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="#1C1D20"
        android:visibility="gone"
        android:id="@+id/statusspace"/>
    <TextView
        android:layout_marginTop="50dp"
        android:layout_marginStart="20dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:textColor="?attr/textColorPrimary"
        android:text="Branding"
        android:id="@+id/main_branding"
        android:textSize="26dp"/>
    <com.news.goodlife.CustomViews.CustomEntries.BorderRoundView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/main_branding"
        app:layout_constraintBottom_toBottomOf="@id/main_branding"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:backgroundCustomColor="#65FFFFFF"
        android:layout_marginEnd="12dp"
        app:borderCustomRadius="100"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/app_title"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/fragment_container_one"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/app_title"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/fragment_container_two"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:id="@+id/previewImage"/>
    <com.news.goodlife.CustomViews.ElasticEdgeView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:elevation="100dp"
        android:outlineProvider="none"
        android:id="@+id/elasticEdge"
        />
        <com.news.goodlife.CustomViews.CustomEntries.BorderRoundView
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toTopOf="@id/menu_container"
            app:layout_constraintLeft_toLeftOf="@id/menu_container"
            app:layout_constraintRight_toRightOf="@id/menu_container"
            app:layout_constraintBottom_toBottomOf="@id/menu_container"
            app:backgroundCustomColor="?attr/menuBackground"
            android:paddingStart="14dp"
            android:paddingEnd="14dp"
            >

            <androidx.cardview.widget.CardView
                android:id="@+id/add_transactions"
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:layout_constraintDimensionRatio="1:1"
                app:cardCornerRadius="40dp"
                app:cardBackgroundColor="?attr/standoutButton"
                app:layout_constraintStart_toEndOf="@id/buttonGoals"
                app:layout_constraintEnd_toStartOf="@id/buttonAnalysis"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                >
                <!--#27282C-->
                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/ic_baseline_add_24"
                    app:tint="?attr/baseBackground"
                    android:padding="10dp"/>
            </androidx.cardview.widget.CardView>


            <com.news.goodlife.CustomViews.CustomIcons.MenuIcons
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="12dp"
                android:id="@+id/buttonWallet"
                android:tag="Wallet"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@id/buttonGoals"/>
            <com.news.goodlife.CustomViews.CustomIcons.MenuIcons
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="12dp"
                android:id="@+id/buttonGoals"
                android:tag="Goals"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toEndOf="@id/buttonWallet"
                app:layout_constraintEnd_toStartOf="@id/add_transactions"/>
            <com.news.goodlife.CustomViews.CustomIcons.MenuIcons
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="12dp"
                android:id="@+id/buttonAnalysis"
                android:tag="Analysis"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toEndOf="@id/add_transactions"
                app:layout_constraintEnd_toStartOf="@id/buttonHub"/>
            <com.news.goodlife.CustomViews.CustomIcons.MenuIcons
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_margin="12dp"
                android:id="@+id/buttonHub"
                android:tag="Hub"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintStart_toEndOf="@id/buttonAnalysis"
                app:layout_constraintEnd_toEndOf="parent"/>
        </com.news.goodlife.CustomViews.CustomEntries.BorderRoundView>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="#8C8C8C"
        android:id="@+id/seperator_line_menu"
        app:layout_constraintBottom_toTopOf="@id/menu_container"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/menu_container"
        />
    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/main_menu_float_open_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:fabSize="mini"
        app:maxImageSize="28dp"
        app:backgroundTint="?attr/standoutButton"
        app:tint="?attr/baseBackground"
        android:scaleType="fitXY"
        android:layout_marginBottom="50dp"
        android:padding="0dp"
        android:src="@drawable/ic_baseline_view_module_custom_24"
        app:layout_constraintBottom_toTopOf="@id/menu_container"
        app:layout_constraintStart_toStartOf="@id/menu_container"
        app:layout_constraintEnd_toEndOf="@id/menu_container"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/statusspace"
        android:text="My Finances"
        android:id="@+id/app_title"
        android:paddingLeft="30dp"
        android:paddingRight="50dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:textSize="30sp"
        android:visibility="gone"
        android:textColor="#2CFFFFFF"/>


</androidx.constraintlayout.widget.ConstraintLayout>