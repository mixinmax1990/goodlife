            android:id="@+id/goal_savings_cont"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_margin="16dp"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Savings reserved for Goals"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toTopOf="@id/goal_saving_module_amount"
                android:id="@+id/goal_savings_module_title"
                android:textSize="11sp"
                android:alpha=".7"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="726,20€"
                android:textSize="24sp"
                android:id="@+id/goal_saving_module_amount"
                app:layout_constraintTop_toBottomOf="@id/goal_savings_module_title"
                app:layout_constraintStart_toStartOf="parent"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>


    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        app:borderCustomRadius="50"
        app:layout_constraintTop_toBottomOf="@id/cashflow_activities_container"
        app:layout_constraintStart_toEndOf="@id/goals_module"
        app:layout_constraintEnd_toEndOf="parent"
