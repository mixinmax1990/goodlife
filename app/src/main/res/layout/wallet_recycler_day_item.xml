<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:elevation="105dp"
    android:outlineProvider="none"
    android:layout_width="match_parent"
    android:paddingTop="0dp"
    android:paddingBottom="50dp"
    android:layout_marginTop="0dp"
    android:layout_marginBottom="0dp">
    <com.news.goodlife.CustomViews.RelationshipMapView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:elevation="200dp"
        android:outlineProvider="none"
        android:id="@+id/overlay_relation_map"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:id="@+id/date_container"
        app:borderCustomRadius="50"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="40dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Monday"
            android:textSize="24sp"
            android:alpha="1"
            android:id="@+id/item_day"
            android:layout_marginTop="20dp"
            android:textColor="?attr/textColorPrimary"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="21 August 2020"
            android:textSize="12sp"
            android:alpha=".6"
            android:textColor="?attr/textColorPrimary"
            android:id="@+id/item_date"
            android:paddingBottom="20dp"
            android:paddingTop="5dp"
            app:layout_constraintBottom_toTopOf="@id/module_status_overview"
            app:layout_constraintTop_toBottomOf="@id/item_day"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>
<com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
