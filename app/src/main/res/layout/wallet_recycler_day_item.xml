    android:layout_width="0dp"
    android:layout_height="0dp"
    app:cardBackgroundColor="#00000000"
    app:borderCustomRadius="50"
    app:layout_constraintStart_toStartOf="parent"
    app:layout_constraintEnd_toStartOf="@id/right_up_frame"
    app:layout_constraintTop_toBottomOf="@id/date_container"
    app:layout_constraintBottom_toBottomOf="@id/right_up_frame"
    app:strokeCustomColor="#00FFFFFF"
    app:strokeCustomSize="1"
    android:padding="16dp"
    android:layout_marginTop="10dp"
    android:layout_marginStart="10dp"
    android:layout_marginEnd="5dp"
    app:cardElevation="0dp"
    >

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/available_title"
        android:text="Total Balance"
        android:alpha=".7"
        android:textSize="12sp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/available_amount"
        android:text="3.775,20€"
        android:textAlignment="center"
        app:layout_constraintTop_toBottomOf="@id/available_title"
       app:layout_constraintStart_toStartOf="parent"
        android:textSize="26sp"/>

    <com.news.goodlife.CustomViews.CustomEntries.LabeledSmallTextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="3dp"
        android:tag="Sparkasse"
        android:paddingTop="15dp"
        android:paddingStart="8dp"
        android:paddingBottom="8dp"
        android:paddingEnd="8dp"
        android:text="2.175,50€"
        android:textSize="12sp"
        android:background="#212226"
        android:textAlignment="center"
        app:cardElevation="0dp"
        app:cardCornerRadius="8dp"
        android:id="@+id/account_one_balance_cont"
        app:layout_constraintTop_toBottomOf="@id/available_amount"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.news.goodlife.CustomViews.CustomEntries.LabeledSmallTextView
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="3dp"
        android:tag="Volksbank"
        android:paddingTop="15dp"
        android:paddingStart="8dp"
        android:paddingBottom="8dp"
        android:paddingEnd="8dp"
        android:text="1.550,50€"
        android:textSize="12sp"
        android:background="#212226"
        android:textAlignment="center"
        app:cardElevation="0dp"
        app:cardCornerRadius="8dp"
        android:id="@+id/account_two_balance_cont"
        app:layout_constraintTop_toBottomOf="@id/account_one_balance_cont"
        app:layout_constraintEnd_toEndOf="parent"/>
</com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>>
    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:1"
        android:id="@+id/right_up_frame"
        app:layout_constraintStart_toEndOf="@id/module_status_overview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/date_container"
        android:layout_marginStart="5dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="5dp"
        />

        <!--<FrameLayout
            android:id="@+id/cashflow_split"
            android:layout_width="20dp"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"/>-->
        <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintStart_toStartOf="@id/right_up_frame"
            app:layout_constraintEnd_toStartOf="@id/events_incoming"
            app:layout_constraintBottom_toBottomOf="@id/right_up_frame"
            app:borderCustomRadius="50"
            android:id="@+id/fixed_costs_module"
            >
           <com.news.goodlife.CustomViews.IconDoughnutView
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:id="@+id/fixed_cost_doughnut"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="70%"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
        </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

        <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintBottom_toBottomOf="@id/right_up_frame"
            app:layout_constraintEnd_toEndOf="@id/right_up_frame"
            app:layout_constraintStart_toEndOf="@id/fixed_costs_module"
            app:backgroundCustomColor="#1a1b1c"
            app:borderCustomRadius="50"
            android:id="@+id/events_incoming"
            >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Einnahmen"
                android:textSize="12sp"
                app:layout_constraintBottom_toTopOf="@id/incoming_value"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:layout_margin="4dp"
                android:alpha=".7"
                android:id="@+id/incoming_title"/>
            <TextView
                android:id="@+id/incoming_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:textColor="?attr/textColorPrimary"
                android:text="0€"
                android:textSize="16sp"
                android:alpha="1"
                android:layout_margin="10dp"
                />
        </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/events_incoming"
        app:layout_constraintTop_toTopOf="@id/right_up_frame"
        app:layout_constraintEnd_toEndOf="@id/events_incoming"
        app:layout_constraintStart_toStartOf="@id/fixed_costs_module"
        android:layout_marginTop="0dp"
        android:layout_marginBottom="10dp"
        android:padding="16dp"
        app:backgroundCustomColor="#000000"
        app:borderCustomRadius="50"
        android:id="@+id/cashcat_frame"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Free to use"
            android:id="@+id/freetouse_title"
            android:alpha=".7"
            android:textSize="12sp"/>
        <TextView
            android:id="@+id/revenue_module_amount"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:elevation="10dp"
            android:text="455,00€"
            android:textSize="26sp"
            app:layout_constraintTop_toBottomOf="@id/freetouse_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:paddingBottom="10dp"
        app:borderCustomRadius="50"
        app:backgroundCustomColor="#00595959"
        android:id="@+id/budget_main_container"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/module_status_overview"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="My Budgets"
            android:layout_marginTop="16dp"
            android:layout_marginStart="16dp"
            android:textSize="12sp"
            android:paddingBottom="8dp"
            android:alpha=".7"
            android:id="@+id/budget_module_title"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <com.news.goodlife.CustomViews.CustomEntries.BorderRoundView
            android:layout_width="0dp"
            android:layout_height="180dp"
            app:layout_constraintTop_toBottomOf="@id/budget_module_title"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:borderCustomRadius="50"
            android:layout_marginStart="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="10dp"
            android:paddingTop="10dp"
            android:paddingBottom="10dp"
            app:backgroundCustomColor="#2A000000"
            android:id="@+id/monthlybudgetcont"
            >
            <com.google.android.flexbox.FlexboxLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:flexDirection="row"
                app:flexWrap="wrap"
                android:id="@+id/month_flex"/>

        </com.news.goodlife.CustomViews.CustomEntries.BorderRoundView>


        <com.google.android.flexbox.FlexboxLayout
            android:orientation="horizontal"
            app:flexDirection="row"
            app:flexWrap="wrap"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/budget_container"
            android:paddingBottom="16dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/budget_module_title"/>

    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:minHeight="150dp"
        app:borderCustomRadius="50"
        app:backgroundCustomColor="#00595959"
        android:id="@+id/cashflow_activities_container"
        app:layout_constraintTop_toBottomOf="@id/budget_main_container"
        android:layout_marginTop="10dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Bank Activities"
            android:textSize="12sp"
            android:padding= "16dp"
            android:alpha=".7"
            android:id="@+id/cash_pop_title"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/no_transactions"
            android:text="No Transactions"
            android:alpha=".7"
            android:textSize="12sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:textColor="#FFFFFF"
            />
        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:flexDirection="column"
            android:layout_marginTop="0dp"
            android:layout_marginBottom="10dp"
            android:paddingBottom="10dp"
            android:alpha="1"
            android:id="@+id/flex_cont"
            app:layout_constraintTop_toBottomOf="@id/cash_pop_title"
            >
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="gone"
                android:id="@+id/day_item_add_cashflow"
                android:padding="40dp"
                >
                <com.news.goodlife.CustomViews.CustomEntries.BorderRoundView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:borderCustomRadius="100"
                    app:strokeCustomSize="2"
                    app:strokeCustomColor="?attr/textColorPrimary"
                    android:alpha=".5"
                    android:id="@+id/add_cash_inday"
                    />
                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:src="@drawable/ic_baseline_add_24"
                    android:alpha=".7"
                    />


            </androidx.constraintlayout.widget.ConstraintLayout>

            <!-- Data Entry Field -->
            <androidx.constraintlayout.widget.ConstraintLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingTop="20dp"
                android:paddingStart="5dp"
                android:paddingEnd="5dp"
                android:visibility="gone"
                android:scaleY=".7"
                android:scaleX=".7"
                android:alpha="0"
                android:id="@+id/add_cashflow_entry_container"
                >
                <com.news.goodlife.CustomViews.CustomEntries.LabeledEntryView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:tag="Amount"
                    app:layout_constraintTop_toTopOf="parent"
                    android:textSize="16sp"
                    android:paddingStart="36dp"
                    android:paddingEnd="25dp"
                    android:paddingTop="25dp"
                    android:paddingBottom="25dp"
                    android:inputType="numberDecimal"
                    android:background="@android:color/transparent"
                    android:id="@+id/day_newentry_amount"/>
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="1:1"
                    android:src="@drawable/ic_baseline_add_24"
                    android:id="@+id/day_newentry_plus"
                    android:padding="15dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintTop_toTopOf="@id/day_newentry_amount"
                    app:layout_constraintBottom_toBottomOf="@id/day_newentry_amount"
                    app:layout_constraintEnd_toEndOf="@id/day_newentry_amount"/>
                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintDimensionRatio="1:1"
                    android:src="@drawable/ic_baseline_remove_24"
                    android:id="@+id/day_newentry_minus"
                    android:padding="15dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginBottom="8dp"
                    app:layout_constraintTop_toTopOf="@id/day_newentry_amount"
                    app:layout_constraintBottom_toBottomOf="@id/day_newentry_amount"
                    app:layout_constraintEnd_toStartOf="@id/day_newentry_plus"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="-"
                    android:id="@+id/day_newentry_minusplus_bf"
                    android:textSize="18sp"
                    app:layout_constraintTop_toTopOf="@id/day_newentry_amount"
                    android:layout_marginStart="20dp"
                    app:layout_constraintStart_toStartOf="@id/day_newentry_amount"
                    app:layout_constraintBottom_toBottomOf="@id/day_newentry_amount"/>
                <com.news.goodlife.CustomViews.CustomEntries.LabeledEntryView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toBottomOf="@id/day_newentry_amount"
                    android:tag="Description"
                    android:textSize="16sp"
                    android:paddingStart="25dp"
                    android:paddingEnd="25dp"
                    android:paddingTop="25dp"
                    android:layout_marginTop="10dp"
                    android:paddingBottom="25dp"
                    android:background="@android:color/transparent"
                    android:id="@+id/day_newentry_description"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10dp"
                    android:id="@+id/add_cashflow_button_save"
                    android:layout_marginEnd="10dp"
                    android:textSize="16sp"
                    android:alpha=".5"
                    android:textAlignment="textEnd"
                    app:layout_constraintTop_toBottomOf="@id/day_newentry_description"
                    android:text="Save"/>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Scan Bill"
                    android:id="@+id/day_scan_bill"
                    app:layout_constraintTop_toBottomOf="@id/day_newentry_description"
                    app:layout_constraintStart_toStartOf="parent"
                    android:textSize="14sp"
                    android:layout_marginStart="16sp"
                    android:padding="10dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

            <ProgressBar
                android:id="@+id/progress_loader"
                style="?android:attr/progressBarStyle"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:padding="20dp"
                android:visibility="gone" />

        </com.google.android.flexbox.FlexboxLayout>
    </com.news.goodlife.CustomViews.CustomEntries.PopUpFrame>

    <com.news.goodlife.CustomViews.CustomEntries.PopUpFrame
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="1:1"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:borderCustomRadius="50"
        app:layout_constraintTop_toBottomOf="@id/cashflow_activities_container"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/savings_module"
        android:visibility="visible"
        android:id="@+id/goals_module">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Savings reserved for Goals"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
